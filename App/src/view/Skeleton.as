		private var _crown		:Sprite;
		private var _eyeLeft	:Sprite;
		private var _eyeRight	:Sprite;
		private var _halo		:Sprite;
		private var _lowerFace	:Sprite;
		private var _mouth		:Sprite;
		private var _neck		:Sprite;
		private var _nose		:Sprite;
		private var _upperFace	:Sprite;
		
		public function Skeleton()
		{
			
